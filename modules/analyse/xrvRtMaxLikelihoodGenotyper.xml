<maxLikelihoodGenotyper>
	<maxLikelihoodPlacerModuleName>xrvRtMaxLikelihoodPlacer</maxLikelihoodPlacerModuleName>
	<useSingleReference>true</useSingleReference>

	<cladeCategory>
		<name>family</name>	
		<displayName>Family</displayName>	
		<whereClause>clade_category = 'family'</whereClause>
		<distanceScalingExponent>-3.0</distanceScalingExponent>
		<distanceCutoff>4.0</distanceCutoff>
		<finalCladeCutoff>60.0</finalCladeCutoff>
	</cladeCategory>

	<cladeCategory>
		<name>clade</name>	
		<displayName>Clade</displayName>	
		<whereClause>clade_category = 'tribe'</whereClause>
		<distanceScalingExponent>-3.0</distanceScalingExponent>
	    <distanceCutoff>2.0</distanceCutoff>
		<finalCladeCutoff>60.0</finalCladeCutoff>		
	</cladeCategory>
	
	<cladeCategory>
		<name>genus</name>	
		<displayName>Genus</displayName>	
		<whereClause>clade_category = 'genus'</whereClause>
		<distanceScalingExponent>-3.0</distanceScalingExponent>
	    <distanceCutoff>1.0</distanceCutoff>
		<finalCladeCutoff>70.0</finalCladeCutoff>		
	</cladeCategory>

	<cladeCategory>
		<name>subgenus</name>	
		<displayName>Subgenus</displayName>	
		<whereClause>clade_category = 'subgenus'</whereClause>
		<distanceScalingExponent>-3.0</distanceScalingExponent>
	    <distanceCutoff>0.6</distanceCutoff>
		<finalCladeCutoff>70.0</finalCladeCutoff>		
	</cladeCategory>

	<cladeCategory>
		<name>species</name>	
		<displayName>Species</displayName>	
		<whereClause>clade_category = 'species'</whereClause>
		<distanceScalingExponent>-3.0</distanceScalingExponent>
	    <distanceCutoff>0.4</distanceCutoff>
		<finalCladeCutoff>70.0</finalCladeCutoff>		
	</cladeCategory>


</maxLikelihoodGenotyper>

